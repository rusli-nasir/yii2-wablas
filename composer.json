{
  "name": "ruslidev/yii2-wablas",
  "description": "Yii2 Extension for https://wablas.com",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Rusli Nasir",
      "email": "rusli.nasir.m@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "*",
    "guzzlehttp/guzzle": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "ruslidev\\wablas\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}